{
  "scripts": {
    "predev": "babel-node tools/startMessage.js",
    "dev": "npm-run-all --parallel open:src test:watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "open:src": "babel-node tools/devServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuildProdWeb": "npm-run-all clean-dist test build:html",
    "buildProd": "npm-run-all buildProdWeb buildProdServer",
    "buildProdWeb": "cross-env NODE_ENV=production babel-node tools/build.js",
    "buildProdServer": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "nodemonProd": "cross-env NODE_ENV=production nodemon dist/server.js",
    "pm2Prod": "cross-env NODE_ENV=production pm2 start dist/server.js"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "history": "^4.6.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^2.6.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.18.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.2",
    "eslint-watch": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "postcss-loader": "^1.3.3",
    "react-router": "^4.0.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
