server {
    listen 80;
    access_log /var/log/nginx/domain1.access.log;

    location / {

        proxy_pass    http://127.0.0.1:3000/;
        proxy_set_header 'test' 'test';
        proxy_cookie_path services example.org;
    }

    location /rest {

        proxy_pass    http://127.0.0.1:5000/services/rest;
        proxy_set_header 'test' 'test';
        proxy_cookie_path services example.org;
    }
}
